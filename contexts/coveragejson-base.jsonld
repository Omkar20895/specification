{
  "@context": {
    "id": "@id",
    "type": "@type",
    "covjson": "http://.../vocab#",
    "w3time": "http://www.w3.org/2006/time#",
    "ssn": "http://www.w3.org/2005/Incubator/ssn/ssnx/ssn#",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "dct": "http://purl.org/dc/terms/",

    "label": { "@id": "skos:prefLabel", "@container": "@language" },
    "title": { "@id": "dct:title", "@container": "@language" },
    "description": { "@id": "dct:description", "@container": "@language" },
    "Instant": "w3time:Instant",
    "dateTime": { "@id": "w3time:inXSDDateTime", "@type": "xsd:dateTime" },
    "Interval": "w3time:ProperInterval",
    "start": "w3time:hasBeginning",
    "end": "w3time:hasEnd",
    "phenomenonTime": "ssn:observationSamplingTime",
    "observedProperty": "ssn:observedProperty",

    "coverages": "covjson:coverage",
    "CoverageCollection": "covjson:CoverageCollection",
    "Parameter": "covjson:Parameter",
    "parameters": { "@id": "covjson:parameter", "@type": "@id", "@container": "@index" },
    "domain": "covjson:domain",
    "ranges": "covjson:rangeSet",
    "RangeSet": "covjson:RangeSet",
    
    "Domain": "covjson:Domain",
    "Grid": "covjson:Grid",
    "Profile": "covjson:Profile",
    "PointSeries": "covjson:PointSeries",
    "Point": "covjson:Point",
    "Trajectory": "covjson:Trajectory",
    "Section": "covjson:Section",
    "MultiPolygonSeries": "covjson:MultiPolygonSeries",
    "MultiPolygon": "covjson:MultiPolygon",
    "Polygon": "covjson:Polygon",
    
    "Coverage": "covjson:Coverage",
    "GridCoverage": "covjson:GridCoverage",
    "ProfileCoverage": "covjson:ProfileCoverage",
    "PointSeriesCoverage": "covjson:PointSeriesCoverage",
    "PointCoverage": "covjson:PointCoverage",
    "TrajectorCoveragey": "covjson:TrajectoryCoverage",
    "SectionCoverage": "covjson:SectionCoverage",
    "MultiPolygonSeriesCoverage": "covjson:MultiPolygonSeriesCoverage",
    "MultiPolygonCoverage": "covjson:MultiPolygonCoverage",
    "PolygonCoverage": "covjson:PolygonCoverage"
  }
}
